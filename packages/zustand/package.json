{
  "name": "@ethan-utils/zustand",
  "version": "1.0.0",
  "description": "封装 Zustand create 函数，一个带有 immer、可选持久化（persist）和选择性订阅（subscribeWithSelector）功能的 zustand store，支持 React 17+",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "scripts": {
    "generate-types": "tsc --emitDeclarationOnly --declaration --declarationDir types --project tsconfig.json",
    "build": "rolldown -c rolldown.config.ts && pnpm generate-types"
  },
  "author": "Ethan Zhang",
  "license": "MIT",
  "keywords": [
    "zustand",
    "state management",
    "react",
    "react state management",
    "react hooks",
    "npm",
    "ethan-utils"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ethanz-code/ethan-utils.git"
  },
  "bugs": {
    "url": "https://github.com/ethanz-code/ethan-utils/issues"
  },
  "homepage": "https://github.com/ethanz-code/ethan-utils#readme",
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "zustand": "^4.5.7"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  }
}
